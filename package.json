{
  "name": "resdjavascript",
  "version": "1.0.0",
  "description": "Javascript files for RESD solution - includes shared validation and utility functions",
  "main": "index.js",
  "scripts": {
    "all": "npm-run-all --parallel gulp serve",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --include-all-sources true mocha -S --compilers js:babel-register test/**/*.js",
    "gulp": "gulp",
    "postcover": "node ./tools/openCoverageReport.js",
    "serve": "serve -s ./build",
    "test": "mocha -S --recursive --compilers js:babel-core/register --reporter mochawesome",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "RESD",
    "Javascript"
  ],
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "author": "SHU - PC1091",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "browserify-css": "^0.10.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-uglify": "^2.1.2",
    "istanbul": "^0.4.5",
    "jsdom": "^10.1.0",
    "mocha": "^3.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mochawesome": "^2.2.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "open": "0.0.5",
    "serve": "^5.1.5",
    "tether-shepherd": "^1.8.1"
  },
  "dependencies": {
    "jquery": "^3.2.1",
    "toastr": "^2.1.2"
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  }
}
