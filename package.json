{
  "name": "resdjavascript",
  "version": "0.0.1",
  "description": "Javascript files for RESD solution - includes shared validation and utility functions",
  "scripts": {
    "start": "npm-run-all --parallel gulp serve",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text --include-all-sources true mocha -S --compilers js:babel-register test/**/*.js",
    "gulp": "gulp",
    "postcover": "node ./tools/openCoverageReport.js",
    "serve": "serve -s ./build",
    "test": "mocha -S --recursive --compilers js:babel-core/register --reporter mochawesome",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "RESD",
    "Javascript"
  ],
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "author": "SHU - PC1091",
  "license": "ISC",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-css": "^0.11.1",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.1",
    "fs": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^3.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^11.1.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "mochawesome": "^2.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^11.1.0",
    "open": "0.0.5",
    "serve": "^6.0.2"
  },
  "dependencies": {
    "fs-extra": "^4.0.0",
    "jquery": "^3.2.1",
    "tether-shepherd": "^1.8.1",
    "toastr": "^2.1.2"
  },
  "browserify-css": "./browserify-css.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hairmot/RESDNPM.git"
  }
}
